{
  "version": 2,
  "builds": [
    {
      "src": "api/gemini.js",
      "use": "@vercel/node"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/gemini",
      "dest": "/api/gemini",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ]
}